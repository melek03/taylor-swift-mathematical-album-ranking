<Page x:Class="tswift_album_ranking.description"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:tswift_album_ranking"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="description">

    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/bg2.png" Stretch="UniformToFill"/>
        </Grid.Background>
        <Frame x:Name="Description" NavigationUIVisibility="Hidden">
            <Frame.Background>
                <ImageBrush ImageSource="/bg22.png" Stretch="UniformToFill"/>
            </Frame.Background>
        </Frame>
        <Button Style="{StaticResource RoundButtonTemplate}" Height="40" BorderBrush="#fff" Name="Continue" Content="Continue" Background="#FF98724C" Margin="134,335,400,75" FontFamily="Franklin Gothic Demi Cond" FontSize="22" Click="ButtonClick" MouseEnter="ContinueEnter" MouseLeave="ContinueLeave" />
    </Grid>
</Page>
